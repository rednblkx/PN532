    int "PN532 MISO GPIO"
	range 0 34
	default 19
	help
		GPIO number (IOxx) for SPI.

config PN532_MOSI
    int "PN532 MOSI GPIO"
	range 0 34
	default 23
	help
		GPIO number (IOxx) for SPI.		

endmenu